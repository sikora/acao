                                           <xs:appinfo>
                                               <xhtml:label>Rede Aquarela</xhtml:label>
                                           </xs:appinfo>
                                       </xs:annotation>
                                   </xs:element>
                                   <!-- End field checkbox-->

                                    <!-- Begin field checkbox -->        
                                    <xs:element name="outros" minOccurs="0" type="xs:boolean">
                                        <xs:annotation>
                                            <xs:appinfo>
                                                <xhtml:label>Outros</xhtml:label>
                                            </xs:appinfo>
                                        </xs:annotation>
                                    </xs:element>
                                    <!-- End field checkbox-->

                                    <!-- Begin field Input Text -->    
                                    <xs:element name="seOutros" minOccurs="0">
                                        <xs:annotation>
                                            <xs:appinfo>
                                                <xhtml:label>Se Outros especifique qual:</xhtml:label>
                                            </xs:appinfo>
                                        </xs:annotation>
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:maxLength value="255" />
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                    <!-- End field Input Text -->                                                         
                        </xs:sequence>  
                    </xs:complexType>
                </xs:element>
                
                <!-- End fieldset -  -->    
