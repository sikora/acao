									<xhtml:label>Situação Escolar</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Nunca frequentou" />
									<xs:enumeration value="Está frequentando" />
