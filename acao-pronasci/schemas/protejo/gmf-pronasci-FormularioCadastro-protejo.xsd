											<xs:sequence>
												<xs:element name="naoPossui" type="xs:boolean" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Não possui</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
												<xs:element name="cegueira" type="xs:boolean" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Cegueira</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="surdez" type="xs:boolean" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Surdez</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
												<xs:element name="mudez" type="xs:boolean" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Mudez</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
												<xs:element name="fisica" type="xs:boolean" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Física</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
												<xs:element name="mental" type="xs:boolean" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Mental</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
												<xs:element name="dificuldadeLocomocao">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Possui alguma dificuldade de locomoção?
															</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>

											</xs:sequence>
										</xs:complexType>
									</xs:element>

								</xs:sequence>
							</xs:complexType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="documentos">
				<xs:annotation>
