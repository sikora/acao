							</xs:annotation>
							<xs:complexType>
								<xs:sequence>

									<xs:element name="cpf" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>CPF*</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="nomeCompleto" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Nome completo*</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="nomeMae" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Nome da mãe*</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="sexo" minOccurs="0">
