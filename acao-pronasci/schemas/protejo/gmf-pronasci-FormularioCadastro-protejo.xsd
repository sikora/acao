															<xhtml:label>Nacionalizado Brasileiro</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
												<xs:element name="portariaNacionalizacao" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Portaria Nacionalização**</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="255" />
