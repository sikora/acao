<?xml version="1.0" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns="http://schemas.fortaleza.ce.gov.br/acao/semas-ficha-identificacao-familia-cras_v2.xsd"
    xmlns:class="http://schemas.fortaleza.ce.gov.br/acao/classificacao.xsd"
    xmlns:idx="http://schemas.fortaleza.ce.gov.br/acao/indexhint.xsd"
    xmlns:xsdext="http://schemas.fortaleza.ce.gov.br/xsdform/extensions.xsd"
    xmlns:xhtml="http://www.w3.org/1999/xhtml"
    targetNamespace="http://schemas.fortaleza.ce.gov.br/acao/semas-ficha-identificacao-familia-cras_v2.xsd"
    elementFormDefault="qualified">

    <xs:element name="formFichaIdentificacaoFamilia" type="formFichaIdentificacaoFamilia">
        <xs:annotation>
            <xs:appinfo>
                <xhtml:label>FICHA DE IDENTIFICAÇÃO DA FAMÍLIA V.2</xhtml:label>
                <class:classificacoes>
                    <class:classificacao>cn=Proteção Social Básica,cn=Assistencia Social</class:classificacao>
                </class:classificacoes>
                <idx:index>
                    <idx:hint key="pessoa.numeronis" xpath="formFichaIdentificacaoFamilia/identificacao/numeronis"/>
                    <idx:hint key="pessoa.codfamiliar" xpath="formFichaIdentificacaoFamilia/identificacao/codfamiliar"/>
                    <idx:hint key="pessoa.nome" xpath="formFichaIdentificacaoFamilia/identresponsavel/nomresp"/>
                    <idx:hint key="pessoa.rg" xpath="formFichaIdentificacaoFamilia/identresponsavel/numrgresp"/>
                    <idx:hint key="pessoa.cpfresp" xpath="formFichaIdentificacaoFamilia/identresponsavel/cpf"/>
                    <idx:hint key="pessoa.nomeMembro" xpath="formFichaIdentificacaoFamilia/imembcomposfam/compfamiliar/nommembro"/>
                    <idx:hint key="pessoa.numNisMembro" xpath="formFichaIdentificacaoFamilia/membcomposfam/compfamiliar/numnismembro"/>
                    <idx:hint key="pessoa.rgMembro" xpath="formFichaIdentificacaoFamilia/membcomposfam/compfamiliar/rgmemb"/>
                    <idx:hint key="pessoa.cpfMembro" xpath="formFichaIdentificacaoFamilia/membcomposfam/compfamiliar/cpfmemb"/>
                </idx:index>
            </xs:appinfo>
        </xs:annotation>
    </xs:element>

	<xs:complexType name="formFichaIdentificacaoFamilia">
        <xs:sequence>

			<xs:element name="identificacao">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>Identificação</xhtml:label>
					</xs:appinfo>
				</xs:annotation>

				<xs:complexType>
					<xs:sequence>

				        <xs:element name="codfamiliar" minOccurs="1">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Código Familiar *</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:integer">
				                    <xs:maxLength value="11"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="numeronis" minOccurs="1">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>NIS *</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:integer">
				                    <xs:maxLength value="11"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="datapreenchi" minOccurs="1" type="xs:date">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Data preenchimento *</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="regionalident"  minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Regional *</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="SER I" />
								<xs:enumeration value="SER II" />
								<xs:enumeration value="SER III" />
								<xs:enumeration value="SER IV" />
								<xs:enumeration value="SER V" />
								<xs:enumeration value="SER VI" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="identcras"  minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>CRAS *</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>

								<xs:restriction base="xs:string">
									<xs:enumeration value="CRAS-BARRA DO CEARÁ" />
									<xs:enumeration value="CRAS-PIRAMBU" />
									<xs:enumeration value="CRAS-LAGAMAR" />
									<xs:enumeration value="CRAS-MUCURIPE" />
									<xs:enumeration value="CRAS-PRAIA DO FUTURO II" />
									<xs:enumeration value="CRAS-SERVILUZ" />
									<xs:enumeration value="CRAS-BELA VISTA" />
									<xs:enumeration value="CRAS-QUINTINO CUNHA" />
									<xs:enumeration value="CRAS-COUTO FERNANDES" />
									<xs:enumeration value="CRAS-SERRINHA" />
									<xs:enumeration value="CRAS-VILA UNIÃO" />
									<xs:enumeration value="CRAS-ARACAPÉ" />
									<xs:enumeration value="CRAS-BOM JARDIM" />
									<xs:enumeration value="CRAS-CONJUNTO ESPERANÇA" />
									<xs:enumeration value="CRAS-GRANJA PORTUGAL" />
									<xs:enumeration value="CRAS-GENIBAÚ" />
									<xs:enumeration value="CRAS-MONDUBIM" />
									<xs:enumeration value="CRAS-CANINDEZINHO" />
									<xs:enumeration value="CRAS-CASTELÃO" />
									<xs:enumeration value="CRAS-CONJUNTO PALMEIRAS" />
									<xs:enumeration value="CRAS-DENDÊ" />
									<xs:enumeration value="CRAS-JARDIM DAS OLIVEIRAS" />
									<xs:enumeration value="CRAS-JOÃO PAULO II" />
									<xs:enumeration value="CRAS-ITINEREANTE" />
								</xs:restriction>

							</xs:simpleType>
						</xs:element>

						<xs:element name="origatend"  minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Origem Atendimento *</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Demanda Espontânea" />
								<xs:enumeration value="Busca Ativa" />
								<xs:enumeration value="Demanda Encaminhada" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="origencam"  minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Origem Encaminhamento</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Assistência Social" />
								<xs:enumeration value="Saúde" />
								<xs:enumeration value="Educação" />
								<xs:enumeration value="Habitação" />
								<xs:enumeration value="Trabalho e Renda" />
								<xs:enumeration value="Direitos Humanos" />
								<xs:enumeration value="Segurança Pública" />
								<xs:enumeration value="Sistema de Garantia de Direito" />
								<xs:enumeration value="ONG" />
								<xs:enumeration value="Outros" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

				        <xs:element name="origencamoutro" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Outros</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

		            </xs:sequence>
		        </xs:complexType>
	        </xs:element>

			<xs:element name="identresponsavel">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>1. Identificação do Responsável</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				
				<xs:complexType>
					<xs:sequence>

				        <xs:element name="nomresp" minOccurs="1">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.1 Nome *</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="naturaResp" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.2 Naturalidade</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="estcivilResp" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.3 Estado Civil</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="numrgresp" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.4 RG </xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="orgaoexpedidor" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Órgão Expedidor</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="dataexpedicaorg" minOccurs="0" type="xs:date">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Data Expedição</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

				        <xs:element name="cpf" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.5 CPF</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                    <xs:pattern value="[0-9]+"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="enderecoresp" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.6 Endereço</xhtml:label>
								</xs:appinfo>
								<xsdext:extensions>
									<xsdext:engine>autoComplete</xsdext:engine>
									<xsdext:service>/enderecos</xsdext:service>
								</xsdext:extensions>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="bairrores" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Bairro</xhtml:label>
								</xs:appinfo>
								<xsdext:extensions>
									<xsdext:engine>autoComplete</xsdext:engine>
									<xsdext:service>/bairros</xsdext:service>
								</xsdext:extensions>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

				        <xs:element name="comunlocalresp" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Comunidade/Localidade</xhtml:label>
				                </xs:appinfo>
								<xsdext:extensions>
									<xsdext:engine>autoComplete</xsdext:engine>
									<xsdext:service>/comunidades</xsdext:service>
								</xsdext:extensions>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="regionalresp"  minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>SER *</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="SER I" />
								<xs:enumeration value="SER II" />
								<xs:enumeration value="SER III" />
								<xs:enumeration value="SER IV" />
								<xs:enumeration value="SER V" />
								<xs:enumeration value="SER VI" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

				        <xs:element name="pontorefresp" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Ponto de Referência</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="telefoneresp"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.7 Telefone</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Próprio" />
								<xs:enumeration value="Comunitário" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

				        <xs:element name="numtelresp" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Nº Telefone</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="recadocontresp" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Recado/Contato</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="sexoresp"  minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.8 Sexo *</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Masculino" />
								<xs:enumeration value="Feminino" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="identgenresp"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.9 Identidade de gênero</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Masculino" />
								<xs:enumeration value="Feminino" />
								<xs:enumeration value="Travesti" />
								<xs:enumeration value="Transsexual" />
								<xs:enumeration value="Trangênero" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="orientsexualresp"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.10 Orientação Sexual</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Homossexualidade/Lesbianidade" />
								<xs:enumeration value="Bissexualidade" />
								<xs:enumeration value="Heterosexualidade" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="corracaresp"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.11 Cor ou Raça</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Preta" />
								<xs:enumeration value="Branca" />
								<xs:enumeration value="Parda" />
								<xs:enumeration value="Amarela" />
								<xs:enumeration value="Indígena" />
								<xs:enumeration value="Sem declaração" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

		            </xs:sequence>
		        </xs:complexType>
	        </xs:element>

			<xs:element name="situacaomoradia">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>2. Situação de Moradia</xhtml:label>
					</xs:appinfo>
				</xs:annotation>

				<xs:complexType>
					<xs:sequence>

						<xs:element name="tipomoradia"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.1 Tipo</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Casa" />
								<xs:enumeration value="Apartamento" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

				        <xs:element name="numcomodos" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>2.2 Número de Cômodos</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="tempocupacao" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>2.3 Tempo de ocupação</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="tembanheiro"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.4 Banheiro</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Sim" />
								<xs:enumeration value="Não" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="afirmatbanheiro"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Caso Afirmativo, encontra-se em</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="dentro do domicílio" />
								<xs:enumeration value="fora do domicílio" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="sitimovel"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.5 Situação Imóvel</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Próprio" />
								<xs:enumeration value="Alugado(a)" />
								<xs:enumeration value="Cedida" />
								<xs:enumeration value="Ocupação ilegal" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="vlraluguel" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Valor do aluguel</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:fractionDigits value='2'/>
										<xs:maxExclusive value="1000000"/>
										<xs:minInclusive value="-1000000"/>
									</xs:restriction>
								</xs:simpleType>
						</xs:element>

						<xs:element name="materialcasa"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.6 Material</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Alvenaria" />
								<xs:enumeration value="Taipa" />
								<xs:enumeration value="Mista" />
								<xs:enumeration value="Outro" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

				        <xs:element name="outromaterialcasa" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Outro material</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="abastecagua"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.7 Abastecimento de água</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Cagece-Tarifa Social" />
								<xs:enumeration value="Cagece-Sem Tarifa Social" />
								<xs:enumeration value="Ligação Clandestina" />
								<xs:enumeration value="Não tem" />
