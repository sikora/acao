        <script type="text/javascript">
            function onload_focus(){
                if( !$.browser.mozilla && !$.browser.webkit)  {
                    $('body').append('<div id="dialog-modal" title="Navegador incompatível" style="display:none;">' +
                            '	<p>Este sistema não está homologado para funcionar no seu navegador. Escolha um dos navegadores abaixo:</p>'+
                            '                <p><a href="http://www.getfirefox.com/">Mozilla Firefox</a> ou <a href="http://chrome.google.com/">Google Chrome</a></p>' +
                            '</div>');
                        $( "#dialog-modal" ).dialog({
                                height: 200,
                                width: 370,
                                modal: true,
                                closeOnEscape: false,
                            open: function(event, ui) { $(".ui-dialog-titlebar-close", this.parentNode).hide(); }
                        });

                }
            }
        </script>
    </head>
    <body onload="onload_focus();">
        <div id='topo'>
            <img src="[% c.uri_for('/static/img/logoBrasaoPMF.png') %]" alt="Brasão da Prefeitura Municipal de Fortaleza"/>
            <div id='topoTitulo'>
                Ação - Sistema de Acompanhamento de Projetos Sociais
                <table>
                    <tr>
                        <th>Administrador:</th>
                        <td><a href="[% c.uri_for_action('/auth/admin/usuario/ver_user',[ c.user.dn ]) %]" title="Alterar Senha">[% c.user.cn %]</a></td>
                    </tr>
                </table>
            </div>
        </div>
        <div id='barraTopo'>
        <div class="contextoPlataforma"> Plataforma Administrativa </div>
            <ul>
                <li style="float: right"><a href="[% c.uri_for_action('/auth/logout') %]" title="Sair">Sair</a></li>
            </ul>
        </div>
        <div id='barraTopoCaminho'>

[% FOREACH item IN breadcrumb %]
          <a href="[% item.url %]" title='[% item.title %]'>[% item.text %]</a> »
[% END %]
