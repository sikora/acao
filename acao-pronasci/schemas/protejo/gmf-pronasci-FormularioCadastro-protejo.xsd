											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="telefone2" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Telefone 2</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="email" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>e-mail</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="nacionalidade" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Nacionalidade*</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="boolNacionalidade" type="xs:boolean" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Brasileiro(a)</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="ufNacionalidade" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>UF Nacionalidade**</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="255" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="municNacionalidade" minOccurs="0">
													<xs:annotation>
