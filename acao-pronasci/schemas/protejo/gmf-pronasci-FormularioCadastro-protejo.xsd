								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="tituloEleitor" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Possui título de eleitor</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="dadosTituloEleitor">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label></xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="numeroTituloEleitor" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Número</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="zonaTituloEleitor" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Zona</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="secaoTituloEleitor" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Seção</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="carteiraHabilitacao" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Possui carteira de habilitação(CNH)?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="servicoMilitar" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Serviço militar obrigatório
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Nunca se alistou" />
									<xs:enumeration value="Foi dispensado" />
									<xs:enumeration value="Está servindo" />
									<xs:enumeration value="Já serviu" />
									<xs:enumeration value="Número certificado reservista" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

