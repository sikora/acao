<?xml version="1.0" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns="http://schemas.fortaleza.ce.gov.br/acao/semas-ficha-atendimento-creas.xsd"
    xmlns:class="http://schemas.fortaleza.ce.gov.br/acao/classificacao.xsd"
    xmlns:idx="http://schemas.fortaleza.ce.gov.br/acao/indexhint.xsd"
    xmlns:xsdext="http://schemas.fortaleza.ce.gov.br/xsdform/extensions.xsd"
    xmlns:xhtml="http://www.w3.org/1999/xhtml"
    targetNamespace="http://schemas.fortaleza.ce.gov.br/acao/semas-ficha-atendimento-creas.xsd"
    elementFormDefault="qualified">

    <xs:element name="formFichaAtendimentoCreas" type="formFichaAtendimentoCreas">
        <xs:annotation>
            <xs:appinfo>
                <xhtml:label>Ficha de Atendimento</xhtml:label>
                <class:classificacoes>
                    <class:classificacao>cn=Prog Enfrentamento a Violação de Direito,cn=Proteção Social Especial,cn=Assistencia Social</class:classificacao>
                    <class:classificacao>cn=Prog Atendimento a População em Situação de Rua,cn=Proteção Social Especial,cn=Assistencia Social</class:classificacao>
                </class:classificacoes>
                <idx:index>
                    <idx:hint key="pessoa.numeronis" xpath="formFichaAtendimentoCreas/numeronis"/>
                    <idx:hint key="pessoa.nome" xpath="formFichaAtendimentoCreas/responsavel/nomresp"/>
                    <idx:hint key="pessoa.numerocpf" xpath="formFichaAtendimentoCreas/numerocpf"/>
                    <idx:hint key="pessoa.numnismembro" xpath="formFichaAtendimentoCreas/compfamiliar/numnismembro"/>
                    <idx:hint key="pessoa.nomemembro" xpath="formFichaAtendimentoCreas/compfamiliar/nommembro"/>
                    <idx:hint key="pessoa.rgmembro" xpath="formFichaAtendimentoCreas/compfamiliar/rgmembro"/>
                    <idx:hint key="pessoa.cpfmembro" xpath="formFichaAtendimentoCreas/compfamiliar/cpfmembro"/>
                </idx:index>
            </xs:appinfo>
        </xs:annotation>
    </xs:element>


	<xs:complexType name="formFichaAtendimentoCreas">
        <xs:sequence>

			<xs:element name="numerocpf" minOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>CPF *</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:maxLength value="11"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="numeronis" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>NIS</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:maxLength value="11"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="demanda"  minOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>Demanda expontânea *</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Sim" />
						<xs:enumeration value="Não" />
				</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="origencaminha" minOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>Origem do Encaminhamento*</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>


			<xs:element name="segmentacao">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>Segmentação</xhtml:label>
					</xs:appinfo>
				</xs:annotation>

				<xs:complexType>
					<xs:sequence>

						<xs:element name="segidoso" minOccurs="0" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Idoso</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="segmulher" minOccurs="0" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Mulher</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="segcriadol" minOccurs="0" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Criança/adolescente</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="segpcb" minOccurs="0" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>PCB [#selecione Tipo Deficiência]</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="seglgbt" minOccurs="0" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>LGBT</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="segdivetracial" minOccurs="0" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Diversidade Étnico-Racial</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="segpessrua" minOccurs="0" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Pessoa em situação de rua</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>


						<xs:element name="segoutr" minOccurs="0" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Outros</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="tipdefic"  minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label># Tipo Deficiência</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Auditiva" />
									<xs:enumeration value="Visual" />
									<xs:enumeration value="Motora" />
									<xs:enumeration value="Mental" />
									<xs:enumeration value="Múltipla" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="responsavel">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>1. Identificação</xhtml:label>
					</xs:appinfo>
				</xs:annotation>

				<xs:complexType>
					<xs:sequence>

				        <xs:element name="nomresp" minOccurs="1">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.1 Nome *</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="apelido" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Apelido</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="nomesocial" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.2 Nome social</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="nompai" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.4 Nome do Pai</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="nommae" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.4.1 Nome da mãe</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="natural" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.5 Naturalidade</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>
				        
				        <xs:element name="dtnasc" minOccurs="1" type="xs:date">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Data de nascimento *</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				        </xs:element>

				        <xs:element name="numrg" minOccurs="1">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.6 RG *</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

				        <xs:element name="orgaoexpedidor" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Órgão Expedidor</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="dataexpedicao" minOccurs="0" type="xs:date">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Data Expedição</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

				        <xs:element name="carteiraprof" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Carteira profissional</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
								</xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="interretdoc"  minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.7 Possui interesse para retirada de documentação? *</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="interetdocqual"  minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.7.1 Qual? *</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Certidão de Nascimento" />
									<xs:enumeration value="RG" />
									<xs:enumeration value="CPF" />
									<xs:enumeration value="CTPS" />
									<xs:enumeration value="Título Eleitor" />
									<xs:enumeration value="Não de aplica" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

				        <xs:element name="estcivil" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.8 Estado Civil</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Solteiro" />
									<xs:enumeration value="Casado" />
									<xs:enumeration value="União estável" />
									<xs:enumeration value="Viúvo" />
									<xs:enumeration value="Separado" />
									<xs:enumeration value="Divorciado" />
								</xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="logradouro" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2. Endereço/Local de Permanência</xhtml:label>
								</xs:appinfo>
								<xsdext:extensions>
									<xsdext:engine>autoComplete</xsdext:engine>
									<xsdext:service>/enderecos</xsdext:service>
								</xsdext:extensions>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="bairro" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Bairro</xhtml:label>
								</xs:appinfo>
								<xsdext:extensions>
									<xsdext:engine>autoComplete</xsdext:engine>
									<xsdext:service>/bairros</xsdext:service>
								</xsdext:extensions>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

				        <xs:element name="comunlocal" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Comunidade/localidade</xhtml:label>
				                </xs:appinfo>
								<xsdext:extensions>
									<xsdext:engine>autoComplete</xsdext:engine>
									<xsdext:service>/comunidades</xsdext:service>
								</xsdext:extensions>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="regionalresp"  minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>SER *</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="SER I" />
								<xs:enumeration value="SER II" />
								<xs:enumeration value="SER III" />
								<xs:enumeration value="SER IV" />
								<xs:enumeration value="SER V" />
								<xs:enumeration value="SER VI" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

				        <xs:element name="pontorefresp" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Ponto de Referência</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>
				        
				        <xs:element name="numtelresp" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>1.10 Telefone</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="foneresp"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Tipo</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Próprio" />
								<xs:enumeration value="Público" />
								<xs:enumeration value="Recado" />
							</xs:restriction>
							</xs:simpleType>
						</xs:element>

				        <xs:element name="recadocontresp" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Pessoa p/ recado</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="orientsex"  minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.11 Orientação Sexual *</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Homossexualidade/Lesbianidade" />
									<xs:enumeration value="Homossexualidade/Lesbianidade" />
									<xs:enumeration value="Bissexualidade" />
									<xs:enumeration value="Heterossexualidade" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="identgenresp"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.12 Identidade de gênero</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Masculino" />
									<xs:enumeration value="Feminino" />
									<xs:enumeration value="Travesti" />
									<xs:enumeration value="Transsexual" />
									<xs:enumeration value="Trangênero" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="corracaresp"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.13 Cor/Raça</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Preta" />
									<xs:enumeration value="Branca" />
									<xs:enumeration value="Parda" />
									<xs:enumeration value="Amarela" />
									<xs:enumeration value="Indígena" />
									<xs:enumeration value="Não declarou" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="religiao"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.14 Religião</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Católica" />
									<xs:enumeration value="Evangélica" />
									<xs:enumeration value="Umbanda" />
									<xs:enumeration value="Outros" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="origem"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.15 Origem</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Fortaleza/centro" />
									<xs:enumeration value="Fortaleza/outros bairros" />
									<xs:enumeration value="Região metropolitana" />
									<xs:enumeration value="Interior do estado" />
									<xs:enumeration value="Outro estado" />
									<xs:enumeration value="Outro país" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

		            </xs:sequence>
		        </xs:complexType>
	        </xs:element>

			<xs:element name="sitsocioecon">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>2. Situação sócioeconômica</xhtml:label>
					</xs:appinfo>
				</xs:annotation>

				<xs:complexType>
					<xs:sequence>

						<xs:element name="profissao"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.1 Profissão</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
						</xs:element>

				        <xs:element name="ocupacao" minOccurs="0">
				            <xs:annotation>
				                <xs:appinfo>
				                    <xhtml:label>Ocupação</xhtml:label>
				                </xs:appinfo>
				            </xs:annotation>
				            <xs:simpleType>
				                <xs:restriction base="xs:string">
				                    <xs:maxLength value="255"/>
				                </xs:restriction>
				            </xs:simpleType>
				        </xs:element>

						<xs:element name="cartassinada"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.2 Carteira assinada</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="tmpcontrib" minOccurs="0" type="xs:integer">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Tempo de contribuição</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="gosttrab" minOccurs="0" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.3 Com que gostaria de trabalhar</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="exercativrem"  minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.4 Exerce alguma atividade remunerada atualmente?</xhtml:label>
								</xs:appinfo>
