                                    <xs:annotation>
                                        <xs:appinfo>
                                            <xhtml:label>Raízes de Cidadania</xhtml:label>
                                        </xs:appinfo>
                                    </xs:annotation>
                                </xs:element>
                                <!-- End field checkbox--> 

                                <!-- Begin field checkbox -->        
                                <xs:element name="ponteEncontro" minOccurs="0" type="xs:boolean">
                                    <xs:annotation>
                                        <xs:appinfo>
                                            <xhtml:label>Ponte de Encontro</xhtml:label>
                                        </xs:appinfo>
                                    </xs:annotation>
                                </xs:element>
                                <!-- End field checkbox--> 


