[% INCLUDE 'auth/registros/digitador/leitura/header.tt' %]
<script SRC="[% c.uri_for('/static/js/input.deflate.plugin.js') %]" type="text/javascript" ></script>
<script SRC="[% c.uri_for('/static/js/regex-mask-plugin.js') %]" type="text/javascript" ></script>
<script SRC="[% c.uri_for('/static/js/jquery.meio.mask.js') %]" type="text/javascript" ></script>
<script SRC="[% c.uri_for('/static/js/xsdForm.js') %]" type="text/javascript" ></script>
<script SRC="[% c.uri_for('/static/js/xsdForm-ui.js') %]" type="text/javascript" ></script>

 <form action="[% c.uri_for_action('/auth/registros/digitador/leitura/store', [ leitura.id_leitura ]) %]" method="post"
    onsubmit="return generateXml('[% c.uri_for_action('/auth/registros/digitador/leitura/xsd', [ leitura.id_leitura ]) %]', this.processed_xml);">
 <input type="hidden" name="processed_xml"/>

 <fieldset>
  <legend>Dados da Leitura</legend>
  <dl>
    <dt>Projeto:</dt>
    <dd>[% leitura.instrumento.projeto.nome %]&nbsp;</dd>
    <dt>Instrumento:</dt>
    <dd>[% leitura.instrumento.nome %]&nbsp;</dd>
    <dt>Coleta inicial:</dt>
    <dd>[% leitura.coleta_ini %]&nbsp;</dd>
    <dt>Coleta final:</dt>
    <dd>[% leitura.coleta_fim %]&nbsp;</dd>
    <dt>Digitação inicial:</dt>
    <dd>[% leitura.digitacao_ini %]&nbsp;</dd>
    <dt>Digitação final:</dt>
    <dd>[% leitura.digitacao_fim %]&nbsp;</dd>
    <dt>Controle:</dt>
    <dd>
	<input type="text" name="controle" />
	<div id="requiredField" style="display: none;">true</div>
    </dd>
  </dl>
 </fieldset>
 
 
 <div id="form_container"></div>

 <input type="submit" name="Submeter"/>

 </form>

 <script type="text/javascript">
   generateForm("[% c.uri_for_action('/auth/registros/digitador/leitura/xsd', [ leitura.id_leitura ]) %]",
                "form_container");
   generateXsdFormUI();
 </script>

[% INCLUDE 'footer.tt' %]
