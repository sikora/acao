                                               <xs:appinfo>
                                                   <xhtml:label>Creas Regional</xhtml:label>
                                               </xs:appinfo>
                                           </xs:annotation>
                                       </xs:element>
                                       <!-- End field checkbox-->   

                                            <!-- Begin field selectBox -->    
                                            <xs:element name="seCREAS"  minOccurs="0">
                                                <xs:annotation>
                                                    <xs:appinfo>
                                                        <xhtml:label>Se CREAS Regional especifique qual:</xhtml:label>
