<?xml version="1.0" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xhtml="http://www.w3.org/1999/xhtml"
	xmlns="http://schemas.fortaleza.ce.gov.br/acao/gmf-pronasci-entrevistaindividual-mulherespaz.xsd"
	xmlns:class="http://schemas.fortaleza.ce.gov.br/acao/classificacao.xsd"
	xmlns:idx="http://schemas.fortaleza.ce.gov.br/acao/indexhint.xsd"
	targetNamespace="http://schemas.fortaleza.ce.gov.br/acao/gmf-pronasci-entrevistaindividual-mulherespaz.xsd"
	elementFormDefault="qualified">

	<xs:element name="formMulheresDaPazEntrevistaIndividual" type="formMulheresDaPazEntrevistaIndividual">
		<xs:annotation>
			<xs:appinfo>
				<xhtml:label>03. ENTREVISTA INDIVIDUAL</xhtml:label>
				<class:classificacoes>
					<class:classificacao>cn=Mulheres da Paz,cn=Infância e Adolescência,cn=Segurança Pública</class:classificacao>
				</class:classificacoes>
				<idx:index>
				    <idx:hint key="pessoa.nome" xpath="formMulheresDaPazAvaliacaoDinamica/estabilidade/nome" />
				    <idx:hint key="pessoa.apelido" xpath="formMulheresDaPazAvaliacaoDinamica/estabilidade/apelido" />
				</idx:index>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="formMulheresDaPazEntrevistaIndividual">
		<xs:sequence>

			<xs:element name="estabilidade" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>1.1 ESTABILIDADE</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>

						<xs:element name="nome" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Nome</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="apelido" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Apelido</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="idade" minOccurs="0" type="xs:integer">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Idade</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="estadoCivil" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Estado civil
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Solteira" />
									<xs:enumeration value="Casada" />
									<xs:enumeration value="União estável" />
									<xs:enumeration value="Viúva" />
									<xs:enumeration value="Divorciada" />
									<xs:enumeration value="Casada" />
									<xs:enumeration value="Separada judicialmente" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="filhos" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Filho(s)
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="quantidadeFilhos" type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quantos?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="idadeFilhos" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quais as idades deles?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ajudaCriancas" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quem ajuda a cuidar das criancas?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="moradia" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Sua casa é:
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Própria" />
									<xs:enumeration value="Alugada" />
									<xs:enumeration value="Cedida" />
									<xs:enumeration value="Coabitada" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ondeMorouAntes" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Onde você morou antes de vir para cá?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="tempoMoradiaAnterior" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Por quanto tempo morou lá?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="tempoMoradiaAtual" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Há quanto tempo mora no bairro?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="pretendePermanecerNobairro" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Pretende permanecer no bairro pelos próximos 12 ou 2 meses?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="parenteNoBairro" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quem da família mora do bairro?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>						

						<!-- Begin fieldset - quemTrabalhaFam -->	
						<xs:element name="quemTrabalhaFam" minOccurs="0" maxOccurs="10">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quem trabalha na sua família e quanto ganha?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="membroFamiliaTrab"  minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Membro da família</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="A própria" />
												<xs:enumeration value="Companheiro(a)" />
												<xs:enumeration value="Enteado(a)" />
												<xs:enumeration value="Genro/Nora" />
												<xs:enumeration value="Neto(a)" />
												<xs:enumeration value="Filho(a)" />
												<xs:enumeration value="Pai/Mãe" />
												<xs:enumeration value="Sogro(a)" />
												<xs:enumeration value="Irmão/Irmã" />
												<xs:enumeration value="Outros Familiares" />
												<xs:enumeration value="Outros não Familiares" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>				
									
									<!-- Begin field selectBox -->	
									<xs:element name="faixaSalarialFam"  minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Faixa Salárial</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Até um quarto de salário mínimo" />
												<xs:enumeration value="Até meio salário mínimo" />
												<xs:enumeration value="Até um salário mínimo" />
												<xs:enumeration value="Até dois salários mínimo" />
												<xs:enumeration value="Até três salários mínimo" />
												<xs:enumeration value="Acima de três salários mínimo" />
											</xs:restriction>										
										</xs:simpleType>
									</xs:element>
									<!-- End field selectBox -->
									
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<!-- END quemTrabalhaFam  -->

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="capcidadeLideranca">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>1.2 CAPACIDADE DE LIDERANÇA</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="conhecimentoBairro">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Se conhece o bairro</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="conhecePostosSaude" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você conhece postos de saúde?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="postosSaudeConhece" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quais?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="conheceConselhosTutelares" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você conhece conselhos tutelares?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="conselhosTutelaresConhece" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quais?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="conhecePracasQuadras" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você conhece praças ou quadras de esportes?
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="pracasQuadrasConhece" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quais?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="conheceDelegacias" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você conhece delegacias no seu bairro?
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="delegaciasConhece" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quais?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="conheceIgrejaTemplos" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você conhece igrejas ou templos no seu bairro?
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="igrejaTemplosConhece" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quais?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

								</xs:sequence>
							</xs:complexType>

						</xs:element>

						<xs:element name="promocaoAtividade">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Se promove/ajuda a promover alguma atividade
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>

									<xs:element name="promoveuFestas" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você já promoveu alguma festa em seu bairro?
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="festasPromoveu" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Qual?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="auxiliouEventosEscola" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você já auxiliou algum evento na escola dos seus filhos?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>									
									<xs:element name="eventosEscola" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Qual?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="auxiliouEventosEsportes" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você já auxiliou algum evento de Esportes?
									</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="eventosEsportes" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Qual?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="auxiliouEventosIgreja" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você já auxiliou algum evento na igreja ou
													templo?
									</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="eventosIgreja" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Qual?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

								</xs:sequence>

							</xs:complexType>

						</xs:element>

						<xs:element name="participacaoEntidadeOuProjeto">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Se participa de entidade ou projeto</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>

									<xs:element name="participaEntidadeProjeto">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você participa de:</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="participaAssociacaoComunitaria" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Associação comunitária
												</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="associacaoComunitaria" type="xs:string" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaPastorais" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Pastorais
												</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>

												<xs:element name="pastorais" type="xs:string" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaConselhos" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Conselhos(comunitários, de defesa social, escolares, outros)</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>

												<xs:element name="conselhos" type="xs:string" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaProjetos" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Projetos</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>

												<xs:element name="projetos" type="xs:string" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaOngs" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>ONGs</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>

												<xs:element name="ongs" type="xs:string" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaCentrosDefesa" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Centros de defesa</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
